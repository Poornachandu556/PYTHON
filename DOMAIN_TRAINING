#another logic of displaying names whos cgpa is > 60 and has 0 arrear
a = ["DINESH","CHARAN","POORNA","BHANU"]
m = [[45,67,88,65],[67,89,45,34],[78,90,56,43],[90,80,70,60]]
per=[]
for i in m:
    t = sum(i)//4
    per.append(t)
for i in range(4):
    print("{}. {} : {}%".format(i+1,a[i],per[i]))
