# flames problem
a = input("b:")
b = input("g:")
a1 = list(a)
b1 = list(b)
for i in range(len(a1)):
    for j in range(len(b1)):
        if(a1[i]==b1[j]):
            a1[i]='*'
            b1[j]='*'
print(a1)
print(b1)
z = 0
for i in range(len(a1)):
    if(a1[i]!="*"):
        z +=1
for i in range(len(b1)):
    if(b1[i]!="*"):
        z +=1
print(z)
f=0
res = list("FLAMES")
for i in range(5):
    f = (f+(z-1))%len(res)
    res.pop(f)
print(res)
