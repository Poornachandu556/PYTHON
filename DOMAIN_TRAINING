#grid methodology pattern
a = [[" " for i in range(10)]for i in range(10)]
for i in range(10):
    for j in range(10):
        if (j==0 or j==9):
            a[i][j]="*"
        elif i==j and i<5:
            a[i][j]="*"
        elif i+j==8 and i<5:
            a[i][j]="*"
