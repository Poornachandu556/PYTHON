'''
def g(a,b):
    while(b!=0):
        rem = a%b
        a=b
        b=rem
    return a
def cp(a,b):
    return g(a,b)==1
n = int(input("enter the number:"))
for i in range(5,n):
    for j in range(4,i):
        for k in range(3,j):
            if(j*j+k*k==i*i and cp(i,j) and cp(i,k) and cp(j,k)):
                print(k,j,i)
